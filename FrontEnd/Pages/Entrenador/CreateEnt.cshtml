@page
@model FrontEnd.Pages.CreateEntModel
@{
    ViewData["Title"]="Create";
}
<h1>Crear Entrenador</h1>
<h3>@ViewData["Mensaje"]</h3>

<div class="form-horizontal">  @* Clase CSS  *@
    <hr />
    <div class="row">  @*  Clase CSS Para manejo de filas *@
        <div class=col-md-4>   @*  Clase CSS Para manejo de Columnas *@
            <form method="POST">
                <div>   </div>
                <div class="form-group">   @*  Clase CSS Para manejo de grupos*@
                    <label asp-for="Entrenador.Documento" class="control-label">  </label>
                    <input asp-for="Entrenador.Documento" class="form-control"/>
                    <label asp-for="Entrenador.Nombres" class="control-label">  </label>
                    <input asp-for="Entrenador.Nombres" class="form-control"/>
                    <label asp-for="Entrenador.Apellidos" class="control-label">  </label>
                    <input asp-for="Entrenador.Apellidos" class="form-control"/>
                    <label asp-for="Entrenador.Genero" class="control-label">  </label>
                    <input asp-for="Entrenador.Genero" class="form-control"/>
                    <label asp-for="Entrenador.DisciplinaDep" class="control-label">  </label>
                    <input asp-for="Entrenador.DisciplinaDep" class="form-control"/>
                    <label asp-for="Entrenador.NumTelf" class="control-label">  </label>
                    <input asp-for="Entrenador.NumTelf" class="form-control"/>
                    <label asp-for="Entrenador.EquipoId" class="control-label">  </label>
                    <input asp-for="Entrenador.EquipoId" class="form-control"/>


                </div>
                <div class="form-group">   @*  Clase CSS Para manejo de grupos *@
                    <input type="submit" value="Crear" class="btn btn-primary" />

                </div>

            </form>

        </div>
    </div>
    <div>
        <a asp-page="EntIndex">Ir al Inicio</a>
    </div>
</div>
@section Scripts{
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}